<%= form_for(@project) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <div class="field">
  	<%= f.text_field :titulo, placeholder: "New Project Title" %>
    <%= f.text_area :content, placeholder: "Describe New Project" %>
  </div>
  <%= f.submit "Post", class: "btn btn-primary" %>
<% end %>